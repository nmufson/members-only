<header>
  <div><h1>Members Only</h1></div>
  <div>
    <nav>
      <% if (!user) { %>
      <!-- User is not logged in -->
      <div><a href="/log-in">Log In</a></div>
      <div><a href="/sign-up">Sign Up</a></div>
      <div><a href="/">Home</a></div>
      <% } else { %>
      <!-- User is logged in -->
      <div>Welcome, <%= user.firstName %>!</div>
      <% if (user.membership_status) { %>
      <!-- User is a member -->
      <div><a href="/">Member Home</a></div>
      <% } else { %>
      <!-- User is logged in but not a member -->
      <div><a href="/">Home</a></div>
      <div><a href="/member-join">Join Club</a></div>
      <% } %>
      <div><button id="openLogOutModalButton">Log Out</button></div>
      <% } %>
    </nav>
  </div>
</header>
